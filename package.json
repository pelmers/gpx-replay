{
    "name": "gpx-replay",
    "version": "0.0.1",
    "description": "Replay GPX files on a map, completely in the client browser",
    "main": "src/server.ts",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "build-dist": "BUILD_MODE=dist webpack --config webpack.config.js",
        "watch": "webpack --config webpack.config.js --watch",
        "format": "node_modules/.bin/prettier --ignore-path .gitignore --write '.' '!**/vendor/**/*' '!dist/**/*'"
    },
    "author": "Peter Elmers",
    "license": "MIT",
    "devDependencies": {
        "@babel/generator": "^7.20.4",
        "@turf/turf": "^6.5.0",
        "@types/mapbox-gl": "^2.7.5",
        "@types/react": "^18.0.20",
        "@types/react-dom": "^18.0.6",
        "css-loader": "^6.7.3",
        "gpxparser": "^3.0.8",
        "makejs.art": "^1.0.2",
        "map.heightgraph": "^0.0.3",
        "mapbox-gl": "^2.10.0",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.2",
        "ts-loader": "^9.3.1",
        "tslint": "^6.1.3",
        "tslint-loader": "^3.5.4",
        "typescript": "^4.8.3",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.6.1",
        "webpack-cli": "^4.10.0"
    }
}
